{
  "name": "@waypointhomes/bookshelf-jsonapi-params-extension",
  "version": "1.1.1",
  "description": "Automatically applies relations, filters, and more from the JSON API spec to your Bookshelf.js results",
  "main": "./dist/index.js",
  "options": {
    "mocha": "--compilers js:babel-core/register --bail test"
  },
  "scripts": {
    "build": "babel -q -L -D ./src/ --out-dir ./dist/",
    "prepublish": "npm run clean && npm run build",
    "test": "./node_modules/.bin/mocha $npm_package_options_mocha",
    "clean": "rm -rf dist/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colonyamerican/bookshelf-jsonapi-params.git"
  },
  "keywords": [
    "bookshelf",
    "knex",
    "jsonapi",
    "json api"
  ],
  "author": {
    "name": "James Dixon",
    "email": "jim.w.dixon@gmail.com",
    "url": "https://github.com/jamesdixon"
  },
  "contributors": [
    {
      "name": "Alec Hirsch",
      "email": "alech1215@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scoutforpets/bookshelf-jsonapi-params/issues"
  },
  "homepage": "https://github.com/scoutforpets/bookshelf-jsonapi-params",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "bluebird": "3.5.4",
    "bookshelf": "0.14.2",
    "chai": "3.5.0",
    "eslint": "2.11.1",
    "eslint-config-hapi": "9.1.0",
    "eslint-plugin-hapi": "4.0.0",
    "knex": "^0.14.6",
    "mocha": "^6.1.4",
    "sqlite3": "4.0.6"
  },
  "engines": {
    "iojs": ">= 1.0.0",
    "node": ">= 0.10"
  },
  "dependencies": {
    "bookshelf-page": "0.3.2",
    "inflection": "^1.12.0",
    "lodash": "^4.17.11",
    "split-string": "^4.0.0"
  }
}
